{
  "name": "project-gorgon-data",
  "version": "0.1.2",
  "description": "Typed game data library for Project Gorgon MMO with Zod schemas",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "import": "./dist/schemas/index.mjs",
      "require": "./dist/schemas/index.cjs"
    },
    "./data": {
      "types": "./dist/data/index.d.ts",
      "import": "./dist/data/index.mjs",
      "require": "./dist/data/index.cjs"
    },
    "./data/items": {
      "types": "./dist/data/items.d.ts",
      "import": "./dist/data/items.mjs",
      "require": "./dist/data/items.cjs"
    },
    "./data/recipes": {
      "types": "./dist/data/recipes.d.ts",
      "import": "./dist/data/recipes.mjs",
      "require": "./dist/data/recipes.cjs"
    },
    "./data/skills": {
      "types": "./dist/data/skills.d.ts",
      "import": "./dist/data/skills.mjs",
      "require": "./dist/data/skills.cjs"
    },
    "./data/abilities": {
      "types": "./dist/data/abilities.d.ts",
      "import": "./dist/data/abilities.mjs",
      "require": "./dist/data/abilities.cjs"
    },
    "./data/npcs": {
      "types": "./dist/data/npcs.d.ts",
      "import": "./dist/data/npcs.mjs",
      "require": "./dist/data/npcs.cjs"
    },
    "./data/areas": {
      "types": "./dist/data/areas.d.ts",
      "import": "./dist/data/areas.mjs",
      "require": "./dist/data/areas.cjs"
    },
    "./data/sources": {
      "types": "./dist/data/sources.d.ts",
      "import": "./dist/data/sources.mjs",
      "require": "./dist/data/sources.cjs"
    },
    "./planner": {
      "types": "./dist/planner/index.d.ts",
      "import": "./dist/planner/index.mjs",
      "require": "./dist/planner/index.cjs"
    },
    "./helpers": {
      "types": "./dist/helpers/index.d.ts",
      "import": "./dist/helpers/index.mjs",
      "require": "./dist/helpers/index.cjs"
    },
    "./match3": {
      "types": "./dist/match3/index.d.ts",
      "import": "./dist/match3/index.mjs",
      "require": "./dist/match3/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown && cp -r src/data/raw dist/data/raw && cp -r src/wikidata dist/wikidata",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "test": "vitest run",
    "test:esm": "node scripts/test-esm.mjs",
    "analyze": "tsx scripts/analyze-json.ts",
    "update-files": "tsx scripts/update-files.ts",
    "diff-data": "tsx scripts/diff-data.ts",
    "update-icons": "tsx scripts/update-icons.ts"
  },
  "keywords": [
    "project-gorgon",
    "mmorpg",
    "game-data"
  ],
  "author": "hunterjarrell",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hjarrell/project-gorgon-data.git"
  },
  "homepage": "https://github.com/hjarrell/project-gorgon-data#readme",
  "bugs": {
    "url": "https://github.com/hjarrell/project-gorgon-data/issues"
  },
  "packageManager": "pnpm@10.8.1",
  "peerDependencies": {
    "zod": "^3.25.0 || ^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "tsdown": "0.21.0-beta.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "zod": "^3.25.0 || ^4.0.0"
  }
}
